import time
import random

def imprime(fila):

    if(len(fila)==0):
        return fila
    
    imagem = fila[0]

    tempo = random.randint(1,10)

    print("Imprimindo {}".format(imagem))

    time.sleep(tempo)

    print("Impresso")
    print("Tempo: {}s".format(tempo))
    print()
    
    fila.pop(0)

    return fila

    

fila = ["IMG1","IMG2","IMG3"]

fila = imprime(fila)
fila = imprime(fila)
fila = imprime(fila)
