class Pilha():

  def __init__(self):
    self.lista = []
  

  def push(self, valor, maximo):
    if(not(maxsize(maximo))):
      self.lista.append(valor)


  def pop(self):
    if(not(self.isEmpty())):
      return self.lista.pop()


  def isEmpty(self):
    return len(self.lista)==0
  
  def maxsize(self, maximo):
    return len(self.lista)==maximo
